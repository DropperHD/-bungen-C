#include <stdio.h>

char * stringToLower(char *string){
for(int i = 0;i < 32;i++) {
    if (string[i] < 97 && string[i] > 64) {
        string[i] = string[i] + 32;
    }
}
    return string;
}


int main() {
char* eingabe[32];
scanf("%s",&eingabe);
printf("%s",stringToLower(eingabe));
    return 0;
}
